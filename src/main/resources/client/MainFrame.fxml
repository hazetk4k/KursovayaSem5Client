<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="506.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.Controller">
   <children>
      <Button fx:id="btnExit" layoutX="285.0" layoutY="459.0" mnemonicParsing="false" onAction="#onBtnExit" prefHeight="26.0" prefWidth="116.0" text="Выход" />
      <TableView fx:id="productTable" editable="true" layoutX="14.0" layoutY="14.0" prefHeight="319.0" prefWidth="658.0">
        <columns>
          <TableColumn fx:id="tblID" prefWidth="50.0" text="id" />
          <TableColumn fx:id="tblName" prefWidth="105.0" text="Название" />
            <TableColumn fx:id="tblFuel" prefWidth="125.0" text="Топливная система" />
            <TableColumn fx:id="tblCarcase" prefWidth="125.0" text="Кузов" />
            <TableColumn fx:id="tblBattery" prefWidth="125.0" text="Аккумулятор" />
            <TableColumn fx:id="tblChassis" prefWidth="125.0" text="Шасси" />
        </columns>
      </TableView>
      <Button fx:id="btnCreate" layoutX="14.0" layoutY="380.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Спроектировать продукт" />
      <Button fx:id="btnDelete" layoutX="14.0" layoutY="420.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Удалить продукт" />
      <Button fx:id="btnAllQuality" layoutX="465.0" layoutY="380.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Общая оценка качества" />
      <Button fx:id="btnProductQuality" layoutX="465.0" layoutY="340.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Оценка качества продукта" />
      <Button fx:id="btnEdit" layoutX="14.0" layoutY="460.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Редактировать продукт" />
      <Button fx:id="btnSaveTxt" layoutX="465.0" layoutY="420.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Сохранить данные продуктов" />
      <Button fx:id="btnLoadTxt" layoutX="465.0" layoutY="459.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Загрузить посленее сохранение" />
      <Button fx:id="btnRefresh" layoutX="14.0" layoutY="340.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="206.0" text="Обновить таблицу продуктов" />
   </children>
</AnchorPane>
